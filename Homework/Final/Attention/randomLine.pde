class randomLine{
  int px,py,x,y;
  randomLine(int px,int py, int x, int y){
    this.px = px;
    this.py = py;
    this.x = x;
    this.y = y;
  }
  
  int getterPx(){
    return this.px;
  }
  
  int getterPy(){
    return this.py;
  }
  
  int getterX(){
    return this.x;
  }
  
  int getterY(){
    return this.y;
  }
}
